<!-- Contact Start -->

<section id="newShowcase" class="contact">
    <div class="display-table">
        <div class="display-content">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="text-center title">
                            <h2 class="text-dark">New<span class="base-color">Education input</span></h2>
                        </div>
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-12 mt-5 contact-form">
                        <form id="contactForm" action="" method="POST" >
                            <div class="row">
                                <!-- degree title -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <input name="degree_title" id="degree_title" type="text" class="form-control"
                                        placeholder="Degree title" onfocus="this.placeholder=''" onblur="this.placeholder='Enter degree title'" required>
                                    </div>
                                </div>
                                <!-- start year -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <select name="startYear" class="yearselect" id="startYear">

                                        </select>
                                    </div>
                                </div>
                                <!-- end year -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <select name="endYear" class="yearselect" id="endYear">

                                        </select>
                                    </div>
                                </div>
                                <!-- cgpa field -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <input name="cgpa" id="cgpa" type="number" step="any" class="form-control"
                                        placeholder="Degree cgpa" onfocus="this.placeholder=''" onblur="this.placeholder='Enter degree cgpa'" required>
                                    </div>
                                </div>
                                <!-- institute -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <input name="institute" id="institute" type="text" class="form-control"
                                        placeholder="Degree institute" onfocus="this.placeholder=''" onblur="this.placeholder='Enter degree erened institute'" required>
                                    </div>
                                </div>
                                <!-- Location -->
                                <div class="col-lg-12 form-item">
                                    <div class="form-group">
                                        <input name="location" id="location" type="text" class="form-control"
                                        placeholder="Degree institute location" onfocus="this.placeholder=''" onblur="this.placeholder='Enter degree erened institute location'" required>
                                    </div>
                                </div>
                                
                                <div class="col-sm-12 mt-4 text-left">
                                    <div class="button-border">
                                        <button type="submit" class="pill-button">Submit</button>
                                    </div>
                                    
                                     
                                    
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="row copy-right">
                    <div class="col-12 text-center">
                        <p class="copy-right">Copyright Â© Ab. </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
